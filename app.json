{
  "expo": {
    "name": "RouteMate",
    "slug": "routemate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icons/mycollection/png/route.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.routemate.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0D0D0D",
        "foregroundImage": "./assets/icons/mycollection/png/route.png"
      },
      "package": "com.routemate.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/icons/mycollection/png/route.png",
      "pwa": {
        "name": "RouteMate",
        "shortName": "RouteMate",
        "description": "Premium Travel Companion & Budget Tracker",
        "backgroundColor": "#0D0D0D",
        "themeColor": "#FFD2AD",
        "display": "standalone",
        "orientation": "portrait",
        "icon": "./assets/icons/mycollection/png/route.png",
        "splash": {
          "image": "./assets/icons/mycollection/png/route.png",
          "resizeMode": "contain",
          "backgroundColor": "#0D0D0D"
        }
      }
    },
    "scheme": "routemate",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics"
    ]
  }
}